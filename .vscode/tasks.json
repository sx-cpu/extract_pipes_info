{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "extract_pipes",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"main.cpp",
				"-std=c++17",
				"extract_pipes.cpp",
				"-o",
				"extract_pipes",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}